<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="NuGet.Lucene.Web:packageMirrorTargetUrl" value="https://www.nuget.org/api/v2/" />
    <add key="NuGet.Lucene.Web:alwaysCheckMirror" value="false" />
    <add key="NuGet.Lucene.Web:allowAnonymousPackageChanges" value="true" />
    <add key="NuGet.Lucene.Web:handleLocalRequestsAsAdmin" value="true" />
    <add key="NuGet.Lucene.Web:localAdministratorApiKey" value="" />
    <add key="NuGet.Lucene.Web:EnableCrossDomainRequests" value="true" />
    <add key="NuGet.Lucene.Web:PackagesPath" value="C:\nuget_lucene\packages" />
    <add key="NuGet.Lucene.Web:LucenePath" value="C:\nuget_lucene\lucene" />
    <add key="NuGet.Lucene.Web:SymbolsPath" value="C:\nuget_lucene\symbols" />
    <add key="NuGet.Lucene.Web:DebuggingToolsPath" value="C:\Program Files (x86)\Windows Kits\10\Debuggers\x86" />
    <add key="NuGet.Lucene.Web:EnablePackageFileWatcher" value="false" />
    <add key="NuGet.Lucene.Web:SynchronizeOnStart" value="false" />
    <add key="NuGet.Lucene.Web:GroupPackageFilesById" value="true" />
    <add key="NuGet.Lucene.Web:packageOverwriteMode" value="Allow" />
    <add key="NuGet.Lucene.Web:luceneMergeFactor" value="10" />
    <add key="NuGet.Lucene.Web:ShowExceptionDetails" value="true" />
  </appSettings>
  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1211">
        <arg key="configType" value="INLINE" />
      </factoryAdapter>
    </logging>
  </common>
  <log4net>
    <appender name="Console" type="log4net.Appender.ManagedColoredConsoleAppender">
      <mapping>
        <level value="ERROR" />
        <foreColor value="White" />
        <backColor value="Red" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="Console" />
    </root>
  </log4net>
